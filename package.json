    "d3": "^7.5.0",
    "p5": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "robotjs": "^0.6.0",
